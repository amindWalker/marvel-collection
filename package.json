{
  "name": "marvel-collection",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "unocss": "unocss \"app/src/**/*.tsx\" -o app/src/App.css -w",
    "dev:unocss": "concurrently 'vite --host' 'unocss \"app/src/**/*.tsx\" -o app/src/App.css -w' ",
    "build:css": "unocss \"src/**/*.tsx\" --out-file=css/uno.css",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.19.0",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.142",
    "@iconify/types": "^2.0.0",
    "@types/node": "^20.9.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "unocss": "^0.57.4",
    "vite": "^5.0.0"
  }
}